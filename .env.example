# === EXEMPLE (NE PAS COMMITTER LES SECRETS) ===
# Active l'authentification basique sur l'UI
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=ReplaceWithAStrongPassword

# Clé d'encryption (important : garder la même pour éviter les erreurs)
N8N_ENCRYPTION_KEY=ReplaceWithALongRandomString

# URL publique de ton instance (à remplacer par ton onrender.com)
WEBHOOK_URL=https://your-service.onrender.com/

# Si tu utilises une base Postgres externe (ou Render Postgres via la Blueprint,
# Render injectera automatiquement les variables DB_POSTGRESDB_*)
DB_TYPE=postgresdb
DB_POSTGRESDB_HOST=hostname
DB_POSTGRESDB_PORT=5432
DB_POSTGRESDB_DATABASE=n8n
DB_POSTGRESDB_USER=username
DB_POSTGRESDB_PASSWORD=secret
